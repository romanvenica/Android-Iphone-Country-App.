<?xml version='1.0' encoding='utf-8'?>
<widget
        id="cordova.plugins.diagnostic.example"
        version="3.2.0"
        xmlns="http://www.w3.org/ns/widgets"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:gap="http://phonegap.com/ns/1.0"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Onyx Blank</name>
    <description>
        Aplicacion Onyx Basica
    </description>
    <author email="romanvenica@gmail.com">
        Roman Venica
    </author>
    <content src="index.html" />
    <access origin="*" />

    <preference name="Fullscreen" value="true" /><!-- Phonegap Build only -->

    <plugin name="cordova-plugin-whitelist" version="*" />
    <plugin name="cordova.plugins.diagnostic" version="*" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.1" />   
    <plugin name="cordova-plugin-device" spec="1.1.4" />
    <plugin name="cordova-plugin-network-information" spec="1.3.1"/>
    <plugin name="cordova-plugin-geolocation" spec="2.4.1" />
    <plugin spec="https://github.com/phonegap/phonegap-mobile-accessibility.git" source="git" />
    <plugin spec="https://github.com/katzer/cordova-plugin-background-mode" source="git" />
    <plugin spec="https://github.com/apache/cordova-plugin-statusbar.git" source="git" />



    <platform name="ios">
        <preference name="DisallowOverscroll" value="true" />
        <preference name="UIWebViewBounce" value="false" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarStyle" value="lightcontent" />
    </platform>

    <platform name="android">
        <preference name="android-targetSdkVersion" value="25" />
    </platform>

</widget>